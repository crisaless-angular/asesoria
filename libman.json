{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.12.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "bootstrap-datepicker@1.8.0",
      "destination": "wwwroot/lib/datepicker"
    },
    {
      "library": "popper.js@1.14.3",
      "destination": "wwwroot/lib/popper.js",
      "files": [
        "umd/popper.js",
        "umd/popper.min.js",
        "umd/popper.min.js.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "gulp@4.0.2",
      "destination": "wwwroot/js/gulp/"
    }
  ]
}